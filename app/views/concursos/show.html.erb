<div class="row">
  <div class="align-content-center">
    <h1>CONCURSO - <%=@concurso.name %></h1>
    <img src="<%=@concurso.image2 %>">
    <p><%=@concurso.description %> </p>
  </div>
</div>
<div class="row">
  <% @concurso.videos.each do |video|  %>
      <div class="col-lg-3 col-md-4 col-sm-6 portfolio-item">
        <div class="card h-100">
          <!--<div class="col-lg-6 col-md-6 col-sm-6">-->
            <div id='myElement<%= video.id%>'>Loading the player...</div>
            <script type='text/javascript'>
                jwplayer("myElement<%= video.id%>").setup({
                    "file": "<%= video.sin_convertir%>"
                });
            </script>
          <!--</div>-->
          <!--<div class="col-lg-6 col-md-6 col-sm-6">-->
            <div id='myElement<%= video.id %>_conv'>Loading the player...</div>
            <script type='text/javascript'>
                jwplayer("myElement<%= video.id%>_conv").setup({
                    "file": "<%= video.sin_convertir%>"
                });
            </script>
          <!--</div>-->
          <div class="card-body">
            <h4 class="card-title">
              <a href="#"><%= video.name%></a>
            </h4>
            <p class="card-text"><%= video.message %></p>
            <p>Enviado por: <%= video.firstNameUser + ' ' + video.lastNameUser %>
            <p>E-mail: <%= video.emailUser %>

            </p>
          </div>
        </div>
      </div>
  <% end %>
</div>
<!-- /.row -->
<!-- Pagination -->
