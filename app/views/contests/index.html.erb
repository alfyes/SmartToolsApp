<% if @concurso.nil? %>
    <div class="row">
      <div class="alert alert-danger align-content-center" role="alert">
        No existe el concurso.
      </div>
    </div>
<% else %>
    <div class="container-fluid">
    <section id="content">

      <div id="our-work">
        <div class="resume_image">
          <a href="<%= '/contests/' + @concurso.concurso_url + '/new' %>"><img src="/img_upload.png" height="64" width="64" alt=""> Subir video</a>
        </div>
        <br>
        <div id="content_videos">
          <ul>
            <% @videos.each do |video| %>
                <% if video.state == 2 %>
                  <li>
                    <div class="link_show_video">
                      <a href="#"><%= video.name%></a>
                    </div>
                    <div id="myElement<%= video.video_id %>">Loading the player...</div>
                    <script type='text/javascript'>
                        jwplayer("myElement<%= video.video_id%>").setup({
                            "file": "<%= url_video_convertido(video) %>"
                        });
                    </script>
                    <div class="resume_image">
                      <div class="item_resume"><strong><%= video.message %></strong></div>
                      <div class="item_resume">Usuario: <%= video.firstNameUser + ' ' + video.lastNameUser %></div>
                      <div class="item_resume">Email: <%= video.emailUser %></div>
                    </div>
                  </li>
                <% end %>
            <% end %>
          </ul>

        </div>
      </div>
      <div class="col-lg-6">

      </div>
    </section>

      <!-- /.row -->
    <!-- Pagination -->
    </div>
<% end %>
